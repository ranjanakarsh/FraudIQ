# Environment variables for FraudIQ platform
# Copy this file to .env and replace with your secure values

# Airflow Fernet Key - Generate with: python utils/generate_fernet_key.py
AIRFLOW_FERNET_KEY=your_generated_fernet_key_here

# MinIO / S3 credentials
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key

# Airflow admin user
AIRFLOW_ADMIN_USER=airflow
AIRFLOW_ADMIN_PASSWORD=your_secure_airflow_password

# PostgreSQL
POSTGRES_USER=airflow
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_DB=airflow
